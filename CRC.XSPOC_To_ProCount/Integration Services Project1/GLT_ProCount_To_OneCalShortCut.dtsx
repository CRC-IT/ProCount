    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{1A113FB5-EF07-4C11-8590-4CBCF6B86D85}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT convert(nvarchar(MAX),&#xA;(&#xA;  &#xA; &quot; + @[User::XSPOCInputSQLQuery]  +  &quot; &#xA; FOR XML PATH ('&quot;+ @[User::MsgType]  +&quot;_Record'), ROOT('&quot;+ @[User::MsgType]  +&quot;_From_&quot; + @[User::PublisherName]  +&quot;')&#xA;&#xA;)) InputMsg&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="ArchiveXMLSQLQuery">
      <DTS:VariableValue
        DTS:DataType="8">SELECT convert(nvarchar(MAX),
(
  
 select cmd.MerrickID as completionID, cm.AreaID, a.areaname
, cmd.RecordDate as activity_date
, cmd.AllocActGravity as gravity
, sum(cmd.AllocActOilVol) as BBL
, sum(cmd.AllocActNGLVol) as GAL
, sum(cmd.AllocActGasVolMCF) as MCF
, cmd.AllocActBTUFactor as BTU_content
, cmd.AllocActPressureBase as BTU_pressure_base
, cmd.AllocActWetDryFlag as BTU_wet_dry_ind
, ISNULL(arg.WellPlusCompletionName,(select WellPlusCompletionName from completiontb with (nolock) where cmd.merrickid = merrickid)) as WellPlusCompletionName
, arg.BusinessEnitity as processing_party
, ltrim(rtrim(arg.locationid)) as rc_user_key
, arg.LocAccountNumber1 as Arg_Det_Sequence
, arg.LocAccountNumber2 as Agreement_ID
, arg.decktype
, 'O' as trans_copy
from CompletionMonthlyDispTb cmd with (nolock) inner join 
CompletionMonthlyTb cm with (nolock) on cmd.MerrickID = cm.merrickID and cmd.RecordDate = cm.RecordDate inner join
(select odi.decktype, odi.objectmerrickid, odi.ObjectMerrickType, odi.activeflag, odi.ownerbeidloc, odi.OwnerBEID, odi.StartDate, odi.EndDate, odi.WellPlusCompletionName,
(select Description from BusinessEntityTb with (nolock) where merrickid = be.MerrickID) BusinessEnitity, be.locationid, be.locaccountnumber1, be.locaccountnumber2
	from BusinessEntityLocationTb be with (nolock) inner join
	(select odi.decktype, odi.objectmerrickid, odi.ObjectMerrickType, od.activeflag, odi.ownerbeidloc, odi.OwnerBEID, max(od.StartDate) as StartDate, max(od.EndDate) as EndDate, 
	(select WellPlusCompletionName from completiontb with (nolock) where odi.objectmerrickid = merrickid and odi.decktype = 15
	) WellPlusCompletionName
	from OwnershipDeckTb od with (nolock) inner join OwnershipDeckInterestTb odi with (nolock) 
	on od.objectmerrickid = odi.objectmerrickid and od.ObjectMerrickType = odi.ObjectMerrickType and od.decktype = odi.decktype and od.StartDate = odi.StartDate
	where odi.decktype = 15 and od.ActiveFlag = 1 and od.ObjectMerrickType = 1 and getdate() between od.StartDate and od.EndDate
	group by odi.decktype, odi.objectmerrickid, odi.ObjectMerrickType, od.activeflag, odi.ownerbeidloc, odi.OwnerBEID) odi 
	on be.LocationNumber = odi.ownerbeidloc and be.merrickid = odi.OwnerBEID) arg on cmd.MerrickID = arg.objectmerrickid
left outer join AreaTb a on cm.AreaID = a.AreaMerrickID and cm.areaid &lt;&gt; 0
where cmd.ProductType = 1 and cmd.SourceType = 2 and cmd.DispositionCode = 5
group by cmd.MerrickID, cm.AreaID, a.areaid, a.areaname
, cmd.RecordDate
, cmd.AllocActGravity
, cmd.AllocActBTUFactor
, cmd.AllocActPressureBase
, cmd.AllocActWetDryFlag
, arg.WellPlusCompletionName 
, arg.BusinessEnitity
, arg.locationid
, arg.LocAccountNumber1
, arg.LocAccountNumber2
,arg.decktype
order by WellPlusCompletionName 
 FOR XML PATH ('GasLoaderTrans_Record'), ROOT('GasLoaderTrans_From_ProCount')

)) InputMsg</DTS:VariableValue>
    </DTS:Variable>
