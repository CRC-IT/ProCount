                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH CTEDAY (ProductType,	DispositionCode,	DispositionCodeDescription,	sourcetype,	completionID,	GatheringSystemID,	AreaID,	AreaName,	GatheringSystemName,	activity_date,	gravity,	BBL,	GAL,	MCF,MMBTU,	BTU_content,	BTU_pressure_base,	BTU_wet_dry_ind,	WellPlusCompletionName,	processing_party,	rc_user_key,	Arg_Det_Sequence,	Agreement_ID,	decktype,	trans_copy,	AccountantPersonID,	AllocationDateStamp,	GatheringSystemLockDate,	GatheringSystemLockName,	GatheringSystemAccountantName)
AS
(
SELECT 
 (SELECT ProductTypeDescription FROM ProductTypeTb WITH (NOLOCK) WHERE MerrickID = cmd.ProductType) AS ProductType
, cmd.DispositionCode
, (SELECT DispositionCodeDescription FROM DispositionCodeTb WITH (NOLOCK) WHERE ProductTypeID = cmd.ProductType AND MerrickID = cmd.DispositionCode) AS DispositionCodeDescription
, cmd.sourcetype
, cmd.MerrickID AS completionID
, cmd.GatheringSystemID
--, gs.GatheringSystemName
, a.AreaID
, a.AreaName
, (SELECT GatheringSystemName FROM GatheringSystemTb WITH (NOLOCK) WHERE GatheringSystemMerrickID = cmd.GatheringSystemID) AS GatheringSystemName 
, cmd.RecordDate AS activity_date
, cmd.AllocActGravity AS gravity
, SUM(cmd.AllocActOilVol) AS BBL
, SUM(cmd.AllocActNGLVol) AS GAL
, SUM(cmd.AllocActGasVolMCF) AS MCF
, SUM(cmd.AllocActGasVolMMBTU) AS MMBTU
, cmd.AllocActBTUFactor AS BTU_content
, cmd.AllocActPressureBase AS BTU_pressure_base
, cmd.AllocActWetDryFlag AS BTU_wet_dry_ind
, ISNULL(arg.WellPlusCompletionName,(SELECT WellPlusCompletionName FROM completiontb WITH (NOLOCK) WHERE cmd.merrickid = merrickid)) AS WellPlusCompletionName
, arg.BusinessEnitity AS processing_party
, LTRIM(RTRIM(arg.locationid)) AS rc_user_key
, arg.LocAccountNumber1 AS Arg_Det_Sequence
, arg.LocAccountNumber2 AS Agreement_ID
, arg.decktype
, 'O' AS trans_copy
--, cmd.AllocationDateStamp
, gs.AccountantPersonID
, cmd.AllocationDateStamp
, gsc.UserDateStamp as GatheringSystemLockDate
,(SELECT (SurName + ', ' + GivenName + ' ' + MiddleInitial) + '(' + UserName + ')' FROM UserTb WITH (NOLOCK) WHERE MerrickID = gsc.UserID) AS GatheringSystemLockName
,(SELECT (SurName + ', ' + GivenName + ' ' + MiddleInitial) + '(' + UserName + ')' FROM UserTb WITH (NOLOCK) WHERE MerrickID = gs.AccountantPersonID) AS GatheringSystemAccountantName
FROM CompletionMonthlyDispTb cmd WITH (NOLOCK) 
INNER JOIN CompletionMonthlyTb cm WITH (NOLOCK) ON cmd.MerrickID = cm.merrickID AND cmd.RecordDate = cm.RecordDate 
INNER JOIN (SELECT 
				odi.decktype
				, odi.objectmerrickid
				, odi.ObjectMerrickType
				, odi.activeflag
				, odi.ownerbeidloc
				, odi.OwnerBEID
				, odi.StartDate
				, odi.EndDate
				, odi.WellPlusCompletionName
				,(SELECT [Description] FROM BusinessEntityTb WITH (NOLOCK) WHERE merrickid = be.MerrickID) BusinessEnitity
				, be.locationid
				, be.locaccountnumber1
				, be.locaccountnumber2
			FROM BusinessEntityLocationTb be WITH (NOLOCK) 
			INNER JOIN 	(SELECT 
							  odi.decktype
							, odi.objectmerrickid
							, odi.ObjectMerrickType
							, od.activeflag
							, odi.ownerbeidloc
							, odi.OwnerBEID
							, MAX(od.StartDate) AS StartDate
							, MAX(od.EndDate) AS EndDate
							,(SELECT WellPlusCompletionName FROM completiontb WITH (NOLOCK) WHERE odi.objectmerrickid = merrickid and odi.decktype = 15) WellPlusCompletionName
						FROM OwnershipDeckTb od WITH (NOLOCK) 
						INNER JOIN OwnershipDeckInterestTb odi WITH (NOLOCK) ON od.objectmerrickid = odi.objectmerrickid AND od.ObjectMerrickType = odi.ObjectMerrickType AND od.decktype = odi.decktype AND od.StartDate = odi.StartDate
						WHERE odi.decktype = 15 and od.ActiveFlag = 1 and od.ObjectMerrickType = 1 and GETDATE() between od.StartDate and od.EndDate
						GROUP BY odi.decktype, odi.objectmerrickid, odi.ObjectMerrickType, od.activeflag, odi.ownerbeidloc, odi.OwnerBEID) odi ON be.LocationNumber = odi.ownerbeidloc AND be.merrickid = odi.OwnerBEID) arg ON cmd.MerrickID = arg.objectmerrickid
LEFT OUTER JOIN AreaTb a WITH (NOLOCK) ON cm.AreaID = a.AreaMerrickID and cm.areaid &lt;&gt; 0
INNER JOIN GatheringSystemTb gs WITH (NOLOCK) ON cmd.GatheringSystemID = gs.GatheringSystemMerrickID
INNER JOIN GatheringSystemCloseTb gsc WITH (NOLOCK) ON cmd.GatheringSystemID = gsc.GatheringSystemID and cmd.RecordDate = gsc.RecordMonth
WHERE cmd.ProductType = 1 AND cmd.SourceType IN (2,3) AND cmd.GatheringSystemID &gt; 0 
AND cmd.DispositionCode IN (SELECT MerrickID FROM DispositionCodeTb WHERE ProductTypeID = 1 AND DispositionGrouping = 1) AND gsc.AllocationLockedFlag = 1
--AND WellPlusCompletionName = 'CRC 8-1'
GROUP BY cmd.MerrickID, cmd.AllocationDateStamp, gs.GatheringSystemName, a.areaid, a.areaname, cmd.DispositionCode, cmd.GatheringSystemID, 
cmd.AllocationDateStamp, gs.AccountantPersonID, gsc.UserDateStamp, gsc.UserID, cmd.ProductType, cmd.sourcetype, cmd.RecordDate, cmd.AllocActGravity, 
cmd.AllocActBTUFactor, cmd.AllocActPressureBase, cmd.AllocActWetDryFlag, arg.WellPlusCompletionName, 
arg.BusinessEnitity, arg.locationid, arg.LocAccountNumber1, arg.LocAccountNumber2, arg.decktype
)

SELECT 
	ProductType
	,ROW_NUMBER() OVER (ORDER BY WellPlusCompletionName ASC) AS Line_No
	,DispositionCode
	,DispositionCodeDescription
	,sourcetype
	,completionID
	,GatheringSystemID
	,AreaID
	,AreaName
	,GatheringSystemName
	,activity_date
	,gravity
	,BBL
	,GAL
	,SUM(MCF) AS MCF
	,SUM(MMBTU) AS MMBTU
	,SUM(MMBTU)/SUM(MCF) BTU_content
	,BTU_pressure_base
	,BTU_wet_dry_ind
	,WellPlusCompletionName
	,processing_party
	,rc_user_key
	,Arg_Det_Sequence
	,Agreement_ID
	,decktype
	,trans_copy
	,'PROCOUNT' AS UserName
	,AccountantPersonID
	,AllocationDateStamp
	,GatheringSystemLockDate
	,GatheringSystemLockName
	,GatheringSystemAccountantName
FROM CTEDAY
GROUP BY 
	ProductType
	,DispositionCode
	,DispositionCodeDescription
	,sourcetype
	,completionID
	,GatheringSystemID
	,AreaID
